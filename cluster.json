{
    "schemaVersion": "1.0.0",
    "class": "Device",
    "async": true,
    "label": "Basic onboarding",
    "Common": {
        "class": "Tenant",
        "hostname": "${local_host}.example.com",
	"myLicense": {
            "class": "License",
            "licenseType": "regKey",
            "regKey": "${regkey}"
        },
        "dbvars": {
            "class": "DbVariables",
            "ui.advisory.enabled": true,
            "ui.advisory.color": "green",
            "ui.advisory.text": "/Common/hostname",
            "config.allow.rfc3927": "enable"
        },
        "myDns": {
            "class": "DNS",
            "nameServers": [
                "${dns_server}",
                "2001:4860:4860::8844"
            ],
            "search": [
                "f5.com"
            ]
        },
        "myNtp": {
            "class": "NTP",
            "servers": [
                "${ntp_server}",
                "0.pool.ntp.org",
                "1.pool.ntp.org"
            ],
            "timezone": "${timezone}"
        },
        "myProvisioning": {
            "class": "Provision",
            "ltm": "nominal",
            "avr": "nominal",
            "asm": "nominal"
        },
        "myManagementRoute": {
            "class": "ManagementRoute",
            "gw": "192.0.2.1",
            "network": "169.254.169.254",
            "mtu": 1500
        },
        "external": {
            "class": "VLAN",
            "tag": 1000,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.1",
                    "tagged": false
                }
            ]
        },
        "external-self": {
            "class": "SelfIp",
            "address": "${local_selfip1}/24",
            "vlan": "external",
            "allowService": "default",
            "trafficGroup": "traffic-group-local-only"
        },
        "ToSC": {
            "class": "VLAN",
            "tag": 1001,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.2",
                    "tagged": false
                }
            ]
        },
        "ToSC-self": {
            "class": "SelfIp",
            "address": "${local_selfip2}/24",
            "vlan": "ToSC",
            "allowService": "default",
            "trafficGroup": "traffic-group-local-only"
        },
        "FrSC": {
            "class": "VLAN",
            "tag": 1002,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.3",
                    "tagged": false
                }
            ]
        },
        "FrSC-self": {
            "class": "SelfIp",
            "address": "${local_selfip3}/24",
            "vlan": "FrSC",
            "allowService": "default",
            "trafficGroup": "traffic-group-local-only"
        },
        "internal": {
            "class": "VLAN",
            "tag": 1003,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.4",
                    "tagged": false
                }
            ]
        },
        "internal-self": {
            "class": "SelfIp",
            "address": "${local_selfip4}/24",
            "vlan": "internal",
            "allowService": "default",
            "trafficGroup": "traffic-group-local-only"
        },        
        "default": {
            "class": "Route",
            "gw": "${gateway}",
            "network": "default",
            "mtu": 1500
        },
        "configsync": {
	    "class": "ConfigSync",
    	    "configsyncIp": "/Common/external-self/address"
	},
	"failoverAddress": {
    	    "class": "FailoverUnicast",
    	    "address": "/Common/external-self/address"
	},
	"failoverGroup": {
	    "class": "DeviceGroup",
	    "type": "sync-failover",
	    "members": ["${host1}.example.com", "${host2}.example.com"],
	    "owner": "/Common/failoverGroup/members/0",
	    "autoSync": true,
	    "saveOnAutoSync": false,
	    "networkFailover": true,
	    "fullLoadOnSync": false,
	    "asmSync": false
	},
	"trust": {
	    "class": "DeviceTrust",
	    "localUsername": "${admin_user}",
	    "localPassword": "${admin_password}",
	    "remoteHost": "${remote_selfip}",
	    "remoteUsername": "${admin_user}",
	    "remotePassword": "${admin_password}"
	}
    }
}
